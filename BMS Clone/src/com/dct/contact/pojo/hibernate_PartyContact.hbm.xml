<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.dct.contact.pojo.PartyContact" table="party_contacts">
      <id name="contactId" column="contact_id">
        <generator class="increment" />
      </id>
       <property name="name"  column="contact_name" />
       <property name="designation"  column="designation" />
       <property name="email1"  column="email1" />
       <property name="email2"  column="email2" />
       <property name="phone1"  column="phone1" />
       <property name="phone2"  column="phone2" />
       <property name="partyId"  column="party_id" />
      </class>
</hibernate-mapping>
